  },
  "main": "lib/index.js",
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@firebase/app-types": "^0.9.0",
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "firebase-admin": "11.5.0",
    "firebase-functions": "4.2.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "firebase-functions-test": "3.0.0",
    "nodemon": "^2.0.22",
    "typescript": "4.9.5"
  },
  "private": true
}
